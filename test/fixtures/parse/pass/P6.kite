module P6

testMain : [Assertion] -> IO (,) {
match {
  asserts ->
    let result = runTest asserts,
        numResults = length results {
      match result {
        Pass -> putLine "#{show showInt numResults} tests passed.",
        Fail (Assertion a) -> bindIO (putLine "FAIL") match {x -> putLine a.message}
      }
    }
  }
}
