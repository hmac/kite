module T6

import T5 (Semigroup, append)

type Foo = Empty | MkFoo

semigroupFoo : Semigroup Foo
semigroupFoo = Semigroup { append = appendFoo }

appendFoo : Foo -> Foo -> Foo
appendFoo Empty f = f
appendFoo MkFoo _ = MkFoo

foo1 : Foo
foo1 = MkFoo

foo2 : Foo
foo2 = append semigroupFoo Empty MkFoo
