class Semigroup a where
  append : a -> a -> a

instance Semigroup String where
  append x y = "#{x}#{y}"

instance Semigroup [a] where
  append [] ys = ys
  append (x :: xs) ys = x :: (append xs ys)

instance Semigroup Int where
  append x y = x + y

