module Simple

data Maybe a = Nothing
             | Just a

data Pair a b = MkPair a b

-- TODO: this should be indented but we can't parse that yet
class Functor f where
map : (a -> b) -> f a -> f b

head : [a] -> a
head [] = error "head: empty list"
head (Cons x xs) = x

const : a -> b -> a
const x y = x

flip : (a -> b -> c) -> b -> a -> c
flip f y x = f x y
